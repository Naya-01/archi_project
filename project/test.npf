%info
WRK2 tests

%config
var_unit={RESULT:us}
var_format={RESULT:%d}


%variables
numRounds={10}
file_size={1024}
key_size={512}
threads={2}
debits={1000}
connection={16}

%script@client 

result=$(../../wrk2-DeathStarBench/wrk http://localhost:8888/?optimized=0 -d30 -c${connection} --latency -t ${threads} -R${debits}  -s ../wrk_scripts/script_${key_size}_${file_size}_${numRounds}.lua)
latency=$(echo "$result" | grep -A 8 ".*Latency.*Distribution" | tail -n 1 | awk '{print $2}')
req_s=$(echo "$result" | grep "Requests/sec" |  awk '{print $2}')
transfer=$(echo "$result" | grep "Transfer/sec" | awk '{print $2}')
nbRequest=$(echo "$result" |tail -n 3 | grep "requests" | awk '{print $1}')

echo "RESULT-LATENCY $latency"
echo "RESULT-REQ $req_s"
echo "RESULT-TRANSFER $transfer"
echo "RESULT-TRANSFER $nbRequest"
